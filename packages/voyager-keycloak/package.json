{
  "name": "@aerogear/voyager-keycloak",
  "version": "0.5.1",
  "description": "Adds Keycloak authentication and authorization to Apollo Voyager Server",
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "homepage": "http://aerogear.org",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aerogear/voyager-server/tree/master/packages/voyager-keycloak"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "test": "ava -v '*.test.ts' '**/*.test.ts'"
  },
  "dependencies": {
    "apollo-server-express": "2.4.8",
    "express-session": "1.15.6",
    "graphql-tools": "4.0.4",
    "joi": "14.3.1",
    "keycloak-connect": "5.0.0",
    "pino": "5.11.3"
  },
  "devDependencies": {
    "@types/express-session": "1.15.12",
    "@types/graphql": "14.0.7",
    "@types/joi": "14.3.2",
    "@types/keycloak-connect": "4.5.1",
    "@types/node": "10.14.3",
    "@types/pino": "5.20.0",
    "ava": "1.4.0",
    "ts-node": "8.0.3",
    "typescript": "3.3.4000"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
